r\n}\r\nexport default contractReducer;","D:\\abdumalik\\grapp\\src\\redux\\reducers\\polisReducer.js",["255","256"],"import { POLIS_ADD, POLIS_CREATE } from '../constants';\r\nconst initialState = [];\r\nconst polisReducer = (polis = initialState, action) => {\r\n\tswitch (action.type) {\r\n\t\tcase POLIS_CREATE:\r\n\t\t\treturn action.payload\r\n\t\t\tbreak;\r\n\t\tcase POLIS_ADD:\r\n\t\t\treturn [\r\n\t\t\t\t...polis, action.payload\r\n\t\t\t];\r\n\t\t\tbreak;\r\n\t\tdefault:\r\n\t\t\treturn polis\r\n\t}\r\n}\r\nexport default polisReducer;","D:\\abdumalik\\grapp\\src\\redux\\reducers\\oplataReducer.js",["257","258","259","260"],"import { OPLATA_ADD, OPLATA_CREATE, OPLATA_DEL} from '../constants';\r\nconst initialState = [];\r\nconst oplataReducer = (oplata = initialState, action) => {\r\n\tswitch (action.type) {\r\n\t\tcase OPLATA_CREATE:\r\n\t\t\treturn action.payload\r\n\t\t\tbreak;\r\n\t\tcase OPLATA_ADD:\r\n\t\t\treturn [\r\n\t\t\t\t...oplata, action.payload\r\n\t\t\t];\r\n\t\t\tbreak;\r\n\t\tcase OPLATA_DEL:\r\n\t\t\treturn [\r\n\t\t\t\t...oplata.filter((item, index)=>index!=action.payload)\r\n\t\t\t];\r\n\t\t\tbreak;\r\n\t\tdefault:\r\n\t\t\treturn oplata\r\n\t}\r\n}\r\nexport default oplataReducer;","D:\\abdumalik\\grapp\\src\\components\\VoditelList.js",[],"D:\\abdumalik\\grapp\\src\\components\\UnvisibleFormElements.js",[],"D:\\abdumalik\\grapp\\src\\helpers\\addDates.js",[],"D:\\abdumalik\\grapp\\src\\helpers\\getCurrentDate.js",[],"D:\\abdumalik\\grapp\\src\\constants\\countries.js",[],"D:\\abdumalik\\grapp\\src\\constants\\paymentTypes.js",[],"D:\\abdumalik\\grapp\\src\\components\\VoditelCreate.js",[],"D:\\abdumalik\\grapp\\src\\redux\\actions\\contract.js",[],"D:\\abdumalik\\grapp\\src\\redux\\actions\\index.js",[],"D:\\abdumalik\\grapp\\src\\screens\\Contract21\\Form.js",["261"],"import React, { useRef, useState } from \"react\";\r\nimport { TopControls } from \"../../components\";\r\nimport Anketa from \"./Anketa\";\r\nimport Contract from \"./Contract\";\r\nimport Payment from \"./Payment\";\r\nimport TransportTable from \"./TransportTable\";\r\n// import Transport from \"../../components/Transport\";\r\nimport Polis from \"./Polis\";\r\nimport \"../../../node_modules/bootstrap-4-grid/css/grid.min.css\";\r\n\r\nconst Form = () => {\r\n    const [active, setActive] = useState(1);\r\n    const [navButtons, setNavButtons] = useState([\r\n        { id: 1, label: 'Общие сведения', isAccessible: true },\r\n        { id: 2, label: 'Объект', isAccessible: true },\r\n        { id: 3, label: 'Договор страхования', isAccessible: true },\r\n        { id: 4, label: 'Оплата', isAccessible: true },\r\n        { id: 5, label: 'Полис', isAccessible: true }\r\n    ])\r\n    const makeAccessible = (index) => {\r\n        const tomodified = [...navButtons]\r\n        const idx = tomodified.findIndex(item => item.id === index)\r\n        tomodified[idx] = { ...tomodified[idx], isAccessible: true }\r\n        setNavButtons([...tomodified])\r\n    }\r\n    const makeActive = (e, id) => {\r\n        e.preventDefault()\r\n        const idx = navButtons.findIndex(item => item.id === id)\r\n        if (navButtons[idx].isAccessible) {\r\n            setActive(id)\r\n        } \r\n    }\r\n    const activeChanger = (e, type) => {\r\n        e.preventDefault()\r\n        const activeId = parseInt(active)\r\n        const idx = navButtons.findIndex(item => item.id === activeId)\r\n        switch (type) {\r\n            case 'increment':\r\n                if (navButtons[idx + 1].isAccessible) {\r\n                    setActive(activeId + 1)\r\n                } \r\n                break;\r\n            case 'decrement':\r\n                if (navButtons[idx - 1].isAccessible) {\r\n                    setActive(activeId - 1)\r\n                }\r\n                break;\r\n            default:\r\n                break;\r\n        }\r\n    }\r\n    \r\n    return (\r\n        <>\r\n            <div className=\"topbar\">\r\n              