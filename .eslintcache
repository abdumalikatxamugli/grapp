is\r\n\t}\r\n}\r\nexport default polisReducer;","D:\\abdumalik\\grapp\\src\\redux\\reducers\\oplataReducer.js",["254","255","256","257"],"import { OPLATA_ADD, OPLATA_CREATE, OPLATA_DEL} from '../constants';\r\nconst initialState = [];\r\nconst oplataReducer = (oplata = initialState, action) => {\r\n\tswitch (action.type) {\r\n\t\tcase OPLATA_CREATE:\r\n\t\t\treturn action.payload\r\n\t\t\tbreak;\r\n\t\tcase OPLATA_ADD:\r\n\t\t\treturn [\r\n\t\t\t\t...oplata, action.payload\r\n\t\t\t];\r\n\t\t\tbreak;\r\n\t\tcase OPLATA_DEL:\r\n\t\t\treturn [\r\n\t\t\t\t...oplata.filter((item, index)=>index!=action.payload)\r\n\t\t\t];\r\n\t\t\tbreak;\r\n\t\tdefault:\r\n\t\t\treturn oplata\r\n\t}\r\n}\r\nexport default oplataReducer;","D:\\abdumalik\\grapp\\src\\components\\VoditelList.js",[],"D:\\abdumalik\\grapp\\src\\components\\UnvisibleFormElements.js",[],"D:\\abdumalik\\grapp\\src\\helpers\\addDates.js",[],"D:\\abdumalik\\grapp\\src\\helpers\\getCurrentDate.js",[],"D:\\abdumalik\\grapp\\src\\constants\\countries.js",[],"D:\\abdumalik\\grapp\\src\\constants\\paymentTypes.js",[],"D:\\abdumalik\\grapp\\src\\components\\VoditelCreate.js",[],"D:\\abdumalik\\grapp\\src\\redux\\actions\\contract.js",[],"D:\\abdumalik\\grapp\\src\\redux\\actions\\index.js",[],"D:\\abdumalik\\grapp\\src\\screens\\Contract21\\Form.js",["258"],"import React, { useRef, useState } from \"react\";\r\nimport { TopControls } from \"../../components\";\r\nimport Anketa from \"./Anketa\";\r\nimport Contract from \"./Contract\";\r\nimport Payment from \"./Payment\";\r\nimport TransportTable from \"./TransportTable\";\r\n// import Transport from \"../../components/Transport\";\r\nimport Polis from \"./Polis\";\r\nimport \"../../../node_modules/bootstrap-4-grid/css/grid.min.css\";\r\n\r\nconst Form = () => {\r\n    const [active, setActive] = useState(1);\r\n    const [navButtons, setNavButtons] = useState([\r\n        { id: 1, label: 'Общие сведения', isAccessible: true },\r\n        { id: 2, label: 'Объект', isAccessible: true },\r\n        { id: 3, label: 'Договор страхования', isAccessible: true },\r\n        { id: 4, label: 'Оплата', isAccessible: true },\r\n        { id: 5, label: 'Полис', isAccessible: true }\r\n    ])\r\n    const makeAccessible = (index) => {\r\n        const tomodified = [...navButtons]\r\n        const idx = tomodified.findIndex(item => item.id === index)\r\n        tomodified[idx] = { ...tomodified[idx], isAccessible: true }\r\n        setNavButtons([...tomodified])\r\n    }\r\n    const makeActive = (e, id) => {\r\n        e.preventDefault()\r\n        const idx = navButtons.findIndex(item => item.id === id)\r\n        if (navButtons[idx].isAccessible) {\r\n            setActive(id)\r\n        } \r\n    }\r\n    const activeChanger = (e, type) => {\r\n        e.preventDefault()\r\n        const activeId = parseInt(active)\r\n        const idx = navButtons.findIndex(item => item.id === activeId)\r\n        switch (type) {\r\n            case 'increment':\r\n                if (navButtons[idx + 1].isAccessible) {\r\n                    setActive(activeId + 1)\r\n                } \r\n                break;\r\n            case 'decrement':\r\n                if (navButtons[idx - 1].isAccessible) {\r\n                    setActive(activeId - 1)\r\n                }\r\n                break;\r\n            default:\r\n                break;\r\n        }\r\n    }\r\n    \r\n    return (\r\n        <>\r\n            <div className=\"topbar\">\r\n                <TopControls search={false} />\r\n                <h1> 21. Страхование транспортных средств, выставляемых в залог</h1>\r\n            </div>\r\n            <div className=\"form-main\">\r\n                <div className=\"stepwizard\">\r\n                    <ul className=\"stepwizard-navigators\">\r\n                        {navButtons.map((item, idx) => (\r\n                            <li className={active === item.id ? 'active' : ''} onClick={(e) => makeActive(e, item.id)} key={idx}>\r\n                                <span>{item.id}</span>\r\n                                <strong>\r\n                                    {item.label}\r\n                                </strong>\r\n                            </li>\r\n                        ))}\r\n                    </ul>\r\n                    <div className=\"d-flex justify-content-around mt-5 mb-5\">\r\n                        {active !== 1 && <button onClick={e => activeChanger(e, 'decrement')}>Отмена</button>}\r\n                        {active !== 5 && <button onClick={e => activeChanger(e, 'increment')}>Далее</button>}\r\n                    </div>\r\n                    {active === 1 && <Anketa  givePermissionToStpep={makeAccessible} />}\r\n                    {active === 2 && <TransportTable  givePermissionToStpep={makeAccessible} />}\r\n                    {active === 3 && <Contract  givePermissionToStpep={makeAccessible} />}\r\n                    {active === 4 && <Payment />}\r\n                    {active === 5 && <Polis />}\r\n                </div>\r\n\r\n            </div>\r\n        </>\r\n    )\r\n}\r\nexport default Form;","D:\\abdumalik\\grapp\\src\\redux\\constants.js",[],"D:\\abdumalik\\grapp\\src\\redux\\actions\\anketa.js",[],"D:\\abdumalik\\grapp\\src\\redux\\actions\\oplata.js",[],"D:\\abdumalik\\grapp\\src\\redux\\actions\\transport.js",[],"D:\\abdumalik\\grapp\\src\\screens\\Contract21\\Anketa.js",["259","260","261"],"import React, { useEffect, useRef, useState } from \"react\";\r\nimport { ClientList, ClientTable, Client, Modal, Countries, Period } from '../../components/';\r\nimport CURRENCYCONDITIONS from '../../constants/currencyConditions';\r\nimport CURRENCIES from '../../constants/currencies';\r\nimport getMaxDate from '../../helpers/getMaxDate';\r\nimport getCurrentDate from '../../helpers/getCurrentDate';\r\nimport countries from \"../../constants/countries\";\r\nimport SimpleReactValidator from 'simple-react-validator';\r\nimport { useDispatch, useSelector } from 